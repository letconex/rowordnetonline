{
   "rewrites": [
    { "source": "/(.*)", "destination": "api/index" }
  ]
  "version": 2,
    "builds": [
        {
            "src": "./api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index"
        }
    ]
}
